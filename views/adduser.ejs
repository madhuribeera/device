<%- include layouts/header.ejs %>


	<div class="page-data">
		<form method="post" enctype="multipart/form-data" action="/usermaster/add">
			<table cellpadding="11">
				<tr>
					<td class="label">Name</td><td>:<input type="text" name="name" placeholder="ex.mobile" required></td>

				</tr>
				<tr>
					<td class="label">Email</td><td>:<input type="text" name="email" placeholder="ex.nani560@gmail.com" required></td>
				</tr>
				<tr>
					<td class="label">Image</td><td>:<input type="file" name="image"  required></td>
				</tr>
				<tr>
					<td class="label">Status</td><td>:<input type="text" name="status" ></td>
				</tr>
				<tr>
					<td class="label">createdBy</td><td>:<input type="text" name="createdBy"></td>

				</tr>
				<tr>
					<td class="label">updatedBy</td><td>:<input type="text" name="updatedBy"></td>

				</tr>

				<tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input type="submit" name="submit" value="Save">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelUser()">
                    </td>
                 </tr>
			</table>
		</form>
	</div>


<%- include layouts/footer.ejs %>