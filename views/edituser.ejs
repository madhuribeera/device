<%- include layouts/header.ejs %>

<div class="page-data">
	<form method="post" enctype="multipart/form-data" action="/usermaster/edit/<%=data[0].userId%>" >
		<table cellpadding="11">
			<tr>
				<td class="label">Name</td><td>:<input type="text" name="name" value="<%= data[0].name %>" required></td>

			</tr>
			<tr>
				<td class="label">Email</td><td>: <input type="text" name="email" value="<%= data[0].email %>" required></td>
			</tr>
			<tr>
				<td class="label">Image</td><td>:<input type="file" name="image"  accept="image/*" ></td>
			</tr>
			<tr>
				<td class="label">Status</td><td>:<input type="text" name="status" ></td>
			</tr>
			<tr>
				<td class="label">createdBy</td><td>:<input type="text" name="createdBy"></td>
			</tr>
			<tr>
				<td class="label">updatedBy</td><td>:<input type="text" name="updatedBy"></td>
			</tr>
			<tr>
                <td class="label"></td>
                <td>
                    &nbsp; <input type="submit" name="submit" value="Save">
                    <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                 </td>
            </tr>
		</table>
	</form>
</div>
<%- include layouts/footer.ejs %>


