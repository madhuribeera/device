<%- include layouts/header.ejs %>

<div Class="page-data">
<div Class="data-btn">
	<button onClick="addUser();">+ Add Developer</button>
</div>
<div class="data-table">
	<table border="2" cellpadding="7" cellspacing="7">
		<tr>
			<th width="50px">No</th>
			<th>FirstName</th>
			<th>LastName</th>
			<th>Email</th>
			<th>Gender</th>
            <th>DOB</th>
			<th width="120px">Action</th>
		</tr>
		<% if(data.length){
			
             for(var i = 0;i < data.length;i++) { %>
				
                <tr>
					<td align="center"><%=(i+1)%></td>
					<td align="center"><%=data[i].firstname%></td>
					<td align="center"><%=data[i].lastname%></td>
					<td align="center"><%=data[i].email%></td>
					<td align="center"><%=data[i].gender%></td>
					<td align="center"><%=data[i].dob%></td>
					<td>
                        <a class="a-inside edit" href="../users/edit/<%=data[i].id%>">Edit</a>
                       
                        <a class="a-inside delete" href="../users/delete/<%=data[i].id%>">Delete</a>
                       
                    </td>
				</tr>
		<% } 
				}else{ %>
				<tr>
					<td colspan="3">No User</td>
				</tr>
		<%	} %>	
	   </table>
    </div>
</div>

<%- include layouts/footer.ejs %>